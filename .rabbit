[server]
port = 8888.0
open = false

[environment]
src = "src"
lib = "lib"
tmp = ".tmp"
dll = ".tmp"
components = "src/components"
images = "src/images"
fonts = "src/fonts"

[ignores]
private = "_"
tmp = ".#"
backup = "~"

[extname]
css = "styl"
html = "html"
js = "js"

[buildList]
strategy = "default"

[dll]
blackList = ["testaaaa"]
failbackStrategy = "ignore"