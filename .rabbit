# -*- mode: toml -*-
#
# Rabbit config file.
#
#
version = "1.0.1"

[server]
port = 8888.0
open = false

[environment]
src = "src"
lib = "lib"
tmp = ".tmp"
dll = ".tmp"
components = "src/components"
images = "src/images"
fonts = "src/fonts"
takerSuffix = "-lib"
injects = "src/__rabbit__"

[ignores]
private = "_"
tmp = ".#"
backup = "~"

[extname]
css = "styl"
html = "html"
js = "js"

[buildList]
strategy = "timeout"
tick = 2000.0
timeout = 5000.0
keepone = true

[dll]
blackList = ["testaaaa", "test1"]
failbackStrategy = "ignore"
